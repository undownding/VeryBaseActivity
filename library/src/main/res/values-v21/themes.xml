<?xml version="1.0" encoding="utf-8"?>
<resources>

	<style name="Theme.AppCompat.TranslucentStatusBar" parent="Base.Theme.AppCompat.FengMoe">
		<item name="android:statusBarColor">@android:color/transparent</item>
	</style>

	<style name="Theme.AppCompat.Light.TranslucentStatusBar" parent="Base.Theme.AppCompat.Light.FengMoe">
		<item name="android:statusBarColor">@android:color/transparent</item>
	</style>

	<style name="Theme.AppCompat.Light.DarkActionBar.TranslucentStatusBar"
	       parent="Base.Theme.AppCompat.Light.DarkActionBar.FengMoe">
		<item name="android:statusBarColor">@android:color/transparent</item>
	</style>


	<style name="Theme.BaseUI.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
		<item name="android:windowDrawsSystemBarBackgrounds">true</item>
		<item name="android:statusBarColor">@android:color/transparent</item>
		<item name="android:windowIsTranslucent">true</item>
	</style>
</resources>